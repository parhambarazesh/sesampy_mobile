<MainScreenView>
    MDBottomNavigation:
        #panel_color: "#eeeaea"
        selected_color_background: "orange"
        text_color_active: "lightgrey"

        MDBottomNavigationItem:
            name: 'pipes'
            icon: 'pipe-disconnected'
            MDBoxLayout:
                id: box
                orientation: 'vertical'
        #        size_hint_y: 0.5
                ScrollView:
                    id: scroll
                    MDList:
                        id: files_list
                        spacing: '8dp'
                        padding: '8dp'
                        MDLabel:
                            id: files
                            text: 'Pipes/Systems'
                            halign: 'center'
                            font_style: 'H6'
                            theme_text_color: 'Primary'
                            size_hint_y: None
                            height: self.texture_size[1]
                        OneLineAvatarIconListItem:
                            id: authenticate
                            text: 'Sesam Authenticate'
                            IconLeftWidget:
                                icon: 'apple-keyboard-command'
                            IconRightWidget:
                                icon: 'subdirectory-arrow-left'
                                theme_text_color: 'Custom'
                                text_color: "blue"
        MDBottomNavigationItem:
            name: 'system'
            icon: 'alpha-s'
            MDBoxLayout:
                id: box
                orientation: 'vertical'
                ScrollView:
                    id: scroll
                    MDList:
                        id: files_list
                        spacing: '8dp'
                        padding: '8dp'
                        MDLabel:
                            id: files
                            text: 'Pipes/Systems'
                            halign: 'center'
                            font_style: 'H6'
                            theme_text_color: 'Primary'
                            size_hint_y: None
                            height: self.texture_size[1]
                        OneLineAvatarIconListItem:
                            id: authenticate
                            text: 'Sesam Authenticate'
                            IconLeftWidget:
                                icon: 'apple-keyboard-command'
                            IconRightWidget:
                                icon: 'subdirectory-arrow-left'
                                theme_text_color: 'Custom'
                                text_color: "blue"
        MDBottomNavigationItem:
            name: 'json'
            icon: 'code-json'
            MDBoxLayout:
                id: box
                orientation: 'vertical'
                ScrollView:
                    id: scroll
                    MDList:
                        id: files_list
                        spacing: '8dp'
                        padding: '8dp'
                        MDLabel:
                            id: files
                            text: 'Pipes/Systems'
                            halign: 'center'
                            font_style: 'H6'
                            theme_text_color: 'Primary'
                            size_hint_y: None
                            height: self.texture_size[1]
                        OneLineAvatarIconListItem:
                            id: authenticate
                            text: 'Sesam Authenticate'
                            IconLeftWidget:
                                icon: 'apple-keyboard-command'
                            IconRightWidget:
                                icon: 'subdirectory-arrow-left'
                                theme_text_color: 'Custom'
                                text_color: "blue"
        MDBottomNavigationItem:
            name: 'show'
            icon: 'eye'
            MDBoxLayout:
                id: box
                orientation: 'vertical'
    MDScreenManager:
        id: screen_manager
        MDScreen:
            id: screen
            MDNavigationDrawer:
                id: nav_drawer
                BoxLayout:
                    orientation: 'vertical'
                    spacing: '8dp'
                    padding: '8dp'
                    Image:
                        source: 'assets/images/sesam_logo.png'
                        size_hint: (1, None)
                        height: Window.height * 0.3
                        pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                    ScrollView:
                        MDList:
                            OneLineIconListItem:
                                text: 'Commands'
                                on_release:
                                    nav_drawer.set_state('close')
                                    app.switch_window('command_screen')
                                    # app.root.current = 'command_screen'
                                    # app.transition.direction = "left"
                                IconLeftWidget:
                                    icon: 'apple-keyboard-command'
                            OneLineIconListItem:
                                text: 'Connectors'
                                on_release:
                                    nav_drawer.set_state('close')
                                    app.switch_window('connector_screen')
#                                   #app.root.current = 'connector_screen'
                                    # call test function
                                    # root.set_up_connectors()
                                IconLeftWidget:
                                    icon: 'connection'
                            OneLineIconListItem:
                                text: 'Configs'
                                on_release:
                                    nav_drawer.set_state('close')
                                    app.switch_window('config_screen')
                                    #app.root.current = 'config_screen'
                                IconLeftWidget:
                                    icon: 'cogs'
                            OneLineIconListItem:
                                text: 'Exit'
                                on_release: app.stop()
                                IconLeftWidget:
                                    icon: 'exit-to-app'

                    MDLabel:
                        text: 'v.1.0.0'
                        font_style: 'Caption'
                        size_hint_y: None
                        height: self.texture_size[1]
                        halign: 'center'