<ConnectorScreenView>
    MDBoxLayout:
        id: box
        orientation: 'vertical'
        ScrollView:
            id: connector_list_view
            size_hint_y: 0.5
            size_hint_x: 1
            spacing: '8dp'
            padding: '8dp'
            MDList:
                id: connector_list
                MDLabel:
                    text: 'Configurations'
                    halign: 'center'
                    font_style: 'H6'
                    theme_text_color: 'Primary'
                    size_hint_y: None
                    height: self.texture_size[1]
        MDLabel:
            id: connector_label
            text: 'Connector'
            font_style: 'H6'
            halign: 'center'


    MDScreenManager:
        MDScreen:
            MDNavigationDrawer:
                id: nav_drawer
                BoxLayout:
                    orientation: 'vertical'
                    spacing: '8dp'
                    padding: '8dp'
                    Image:
                        source: 'assets/images/sesam_logo.png'
                        size_hint: (1, None)
                        height: Window.height * 0.3
                        pos_hint: {'center_x': 0.5, 'center_y': 0.5}
                    ScrollView:
                        MDList:
                            OneLineIconListItem:
                                text: 'Commands'
                                on_release:
                                    nav_drawer.set_state('close')
                                    app.switch_window('command_screen')
                                    # app.root.current = 'command_screen'
                                    # app.root.manager.transition.direction = "left"
                                IconLeftWidget:
                                    icon: 'apple-keyboard-command'
                            OneLineIconListItem:
                                text: 'Connectors'
                                on_release:
                                    nav_drawer.set_state('close')
                                    app.switch_window('connector_screen')
                                    #app.root.current = 'connector_screen'
                                    # call test function
                                    # root.set_up_connectors()
                                IconLeftWidget:
                                    icon: 'connection'
                            OneLineIconListItem:
                                text: 'Configs'
                                on_release:
                                    nav_drawer.set_state('close')
                                    app.switch_window('config_screen')
                                    #app.root.current = 'config_screen'
                                IconLeftWidget:
                                    icon: 'cogs'
                            OneLineIconListItem:
                                text: 'Exit'
                                on_release: app.stop()
                                IconLeftWidget:
                                    icon: 'exit-to-app'

                    MDLabel:
                        text: 'v.1.0.0'
                        font_style: 'Caption'
                        size_hint_y: None
                        height: self.texture_size[1]
                        halign: 'center'